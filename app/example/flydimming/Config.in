config AOS_APP_FLYDIMMING
    bool "Fly Dimming Application"
    select AOS_COMP_CLI if !AOS_BOARD_ESP8266
    select AOS_COMP_NETMGR
    select AOS_COMP_OTA
    select AOS_COMP_CJSON
    select AOS_COMP_LINKKIT_SDK_C
    select APP_FEATURE_LINKKIT
    select AOS_COMP_CJSON
    help
        fly dimming

if AOS_APP_FLYDIMMING
# Configurations for app flydimming
choice
    prompt "Select Case"
    default FLYDIMMING_CONFIG_CASE_SOLO
    help
        select case to build

    config FLYDIMMING_CONFIG_CASE_SOLO
        bool "SOLO"
        help
            default case, build fly_dimming.c
endchoice

endif
